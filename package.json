{
  "name": "nabha-shiksha-ai",
  "version": "1.0.0",
  "description": "AI-powered digital learning platform for rural students in Punjabi",
  "main": "server/index.js",
  "homepage": "https://prabhu-bit.github.io/Digital-platform-for-students/",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d client/build"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "compression": "^1.7.4",
    "compromise": "^14.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "multer": "^1.4.5-lts.1",
    "natural": "^6.7.0",
    "node-cron": "^3.0.2",
    "openai": "^4.20.1",
    "sentiment": "^5.0.2",
    "sqlite3": "^5.1.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "gh-pages": "^6.3.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "education",
    "ai",
    "punjabi",
    "offline-first",
    "voice-recognition",
    "rural-education"
  ],
  "author": "Nabha Shiksha Team",
  "license": "MIT"
}

